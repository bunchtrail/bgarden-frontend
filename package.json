{
  "name": "bgarden-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config vite.http.config.ts",
    "dev:https": "vite",
    "dev:http": "vite --config vite.http.config.ts",
    "build": "tsc -b && vite build --config vite.http.config.ts",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate-certs": "node ./scripts/generate-certs.mjs",
    "generate-trusted-certs": "node ./scripts/generate-trusted-certs.mjs",
    "install-cert-windows": ".\\scripts\\install-cert-windows.bat",
    "start:https": "npm run generate-certs && npm run dev:https",
    "start:trusted-https": "npm run generate-trusted-certs && npm run dev:https",
    "start:http": "npm run dev:http"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.3.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.6",
    "@mui/material": "^6.4.6",
    "@reduxjs/toolkit": "^2.6.0",
    "@types/jsonwebtoken": "^9.0.9",
    "axios": "^1.8.1",
    "jsonwebtoken": "^9.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.0.9",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "mkcert": "^3.2.0",
    "node-forge": "^1.3.1",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
